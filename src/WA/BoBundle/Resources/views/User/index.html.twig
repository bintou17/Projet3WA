{% extends '::base.html.twig' %}

{% block body -%}
    <h1>User list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>Username</th>
                <th>Password</th>
                <th>Ville</th>
                <th>Zipcode</th>
                <th>Tel</th>
                <th>Ip</th>
                <th>Enabled</th>
                <th>Lastlogin</th>
                <th>Expired</th>
                <th>Locked</th>
                <th>Usernamecanonical</th>
                <th>Emailcanonical</th>
                <th>Salt</th>
                <th>Expiresat</th>
                <th>Confirmationtoken</th>
                <th>Passwordrequestedat</th>
                <th>Googleid</th>
                <th>Googleaccesstoken</th>
                <th>Flickrid</th>
                <th>Flickraccesstoken</th>
                <th>Githubid</th>
                <th>Githubaccesstoken</th>
                <th>Linkedinaccesstoken</th>
                <th>Linkedinid</th>
                <th>Roles</th>
                <th>Extras</th>
                <th>Longitude</th>
                <th>Latitude</th>
                <th>Facebookid</th>
                <th>Facebookaccesstoken</th>
                <th>Twitterid</th>
                <th>Twitteraccesstoken</th>
                <th>Credentialsexpired</th>
                <th>Credentialsexpireat</th>
                <th>Createdat</th>
                <th>Updatedat</th>
                <th>Deletedat</th>
                <th>Lastactivity</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('user_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.email }}</td>
                <td>{{ entity.username }}</td>
                <td>{{ entity.password }}</td>
                <td>{{ entity.ville }}</td>
                <td>{{ entity.zipcode }}</td>
                <td>{{ entity.tel }}</td>
                <td>{{ entity.ip }}</td>
                <td>{{ entity.enabled }}</td>
                <td>{% if entity.lastLogin %}{{ entity.lastLogin|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.expired }}</td>
                <td>{{ entity.locked }}</td>
                <td>{{ entity.usernameCanonical }}</td>
                <td>{{ entity.emailCanonical }}</td>
                <td>{{ entity.salt }}</td>
                <td>{% if entity.expiresAt %}{{ entity.expiresAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.confirmationToken }}</td>
                <td>{% if entity.passwordRequestedAt %}{{ entity.passwordRequestedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.googleid }}</td>
                <td>{{ entity.googleaccesstoken }}</td>
                <td>{{ entity.flickrid }}</td>
                <td>{{ entity.flickraccesstoken }}</td>
                <td>{{ entity.githubid }}</td>
                <td>{{ entity.githubaccesstoken }}</td>
                <td>{{ entity.linkedinaccesstoken }}</td>
                <td>{{ entity.linkedinid }}</td>
                <td>{{ entity.roles }}</td>
                <td>{{ entity.extras }}</td>
                <td>{{ entity.longitude }}</td>
                <td>{{ entity.latitude }}</td>
                <td>{{ entity.facebookid }}</td>
                <td>{{ entity.facebookaccesstoken }}</td>
                <td>{{ entity.twitterid }}</td>
                <td>{{ entity.twitteraccesstoken }}</td>
                <td>{{ entity.credentialsExpired }}</td>
                <td>{% if entity.credentialsExpireAt %}{{ entity.credentialsExpireAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.createdAt %}{{ entity.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.updatedAt %}{{ entity.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.deletedAt %}{{ entity.deletedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.lastactivity %}{{ entity.lastactivity|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('user_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('user_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('user_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
